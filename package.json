{
  "name": "psicopedia-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@date-io/moment": "1.3.8",
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fortawesome/react-fontawesome": "0.1.8",
    "@material-ui/core": "4.2.1",
    "@material-ui/icons": "4.2.1",
    "@material-ui/pickers": "3.2.0",
    "@types/jest": "24.0.15",
    "@types/material-ui": "0.21.6",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.1.1",
    "@types/react-router-dom": "4.3.4",
    "@types/react-select": "3.0.5",
    "@types/redux-actions": "2.6.1",
    "@types/styled-components": "4.1.18",
    "axios": "0.19.0",
    "bootstrap": "4.4.1",
    "clsx": "1.0.4",
    "env-cmd": "10.0.1",
    "i18next": "17.0.6",
    "i18next-browser-languagedetector": "3.0.1",
    "jquery": "1.9.1 - 3",
    "moment": "2.24.0",
    "node-sass": "4.13.0",
    "popper.js": "1.16.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hook-form": "3.29.4",
    "react-hooks": "1.0.1",
    "react-i18next": "10.11.4",
    "react-idle-timer": "4.2.9",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "react-select": "3.0.8",
    "redux-actions": "2.6.5",
    "redux-devtools-extension": "2.13.8",
    "redux-saga": "1.1.1",
    "redux-thunk": "2.3.0",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "env-cmd -f develop.env npm run start",
    "build": "react-scripts build",
    "build:noprod": "env-cmd -f develop.env npm run build",
    "test": "react-scripts test --watchAll=false --bail=1 --colors",
    "coverage": "react-scripts test --coverage --watchAll=false --coverageDirectory=output/coverage/ --colors",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/definitionfiles/**",
      "!**/WebWrokers/**",
      "!**/*.mock.ts",
      "!src/setupTests.ts"
    ],
    "coverageReporters": [
      "text",
      "html",
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "axios-mock-adapter": "1.17.0",
    "concurrently": "5.0.0",
    "react-app-env": "1.2.3",
    "sonar-scanner": "3.1.0"
  }
}
